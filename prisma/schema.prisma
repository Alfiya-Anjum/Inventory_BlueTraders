generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Sale {
  id            Int      @id @default(autoincrement())
  productName   String
  company       String
  productNumber String
  basePrice     Float
  discount      Float
  gstPercentage Float
  totalPrice    Float
  createdAt     DateTime @default(now())
}
